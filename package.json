    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^6.1.0",
    "history": "^5.3.0",
    "jest": "^29.7.0",
    "jest-styled-components": "^7.0.4",
    "moxios": "^0.4.0",
    "mq-polyfill": "^1.1.8",
    "prettier": "^2.3.0",
    "react-select-event": "^5.5.1",
    "redux-mock-store": "^1.5.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ]
  }
