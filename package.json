{
  "name": "eshopping-app-client",
  "version": "0.1.0",
  "private": true,
  "description": "Client part of eshopping app",
  "homepage": "https://radeon2211.github.io/eshopping-client",
  "dependencies": {
    "axios": "^1.6.2",
    "axios-rate-limit": "^1.3.0",
    "country-codes-list": "^1.6.11",
    "filesize": "^10.1.0",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.16",
    "moment": "^2.30.1",
    "prop-types": "^15.7.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet": "^6.1.0",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.21.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.8.0",
    "react-textarea-autosize": "^8.5.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.3.0",
    "round-to": "^6.0.0",
    "styled-components": "^6.1.3",
    "uuid": "^9.0.1",
    "yup": "^1.3.3"
  },
  "scripts": {
    "start": "set \"BROWSER=none\" && react-scripts start",
    "start:local": "env-cmd -e dev npm run-script start",
    "build": "set \"GENERATE_SOURCEMAP=false\" &&  react-scripts build",
    "build:prod": "env-cmd -e prod npm run-script build",
    "test": "react-scripts test",
    "test:unit": "env-cmd -e dev npm run-script test",
    "test:e2e": "npx cypress open",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.9",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-router-dom": "^5.3.3",
    "axios-mock-adapter": "^1.22.0",
    "cypress": "^13.6.1",
    "cz-conventional-changelog": "^3.3.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.0",
    "history": "^5.3.0",
    "jest": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "moxios": "^0.4.0",
    "mq-polyfill": "^1.1.8",
    "prettier": "^3.2.4",
    "react-select-event": "^5.5.1",
    "redux-mock-store": "^1.5.4",
    "typescript": "^5.3.3",
    "uuid-validate": "^0.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|query-string|decode-uri-component|split-on-first|filter-obj)/)"
    ]
  },
  "engines": {
    "node": "20.x"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    }
  }
}
