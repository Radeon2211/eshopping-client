    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "query-string": "^7.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^4.0.11",
    "react-helmet": "^6.1.0",
    "react-outside-click-handler": "^1.3.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.8.0",
    "react-textarea-autosize": "^8.5.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.3.0",
